{
  "name": "react-youtube-music-player",
  "version": "1.0.5",
  "description": "A simple React hook for building a music player using the official YouTube IFrame Player API",
  "author": "Carl-Robert Linnupuu",
  "license": "ISC",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir lib/esm --lib es6,dom -m es2015",
    "build:cjs": "tsc --outDir lib/cjs --lib es6,dom -m commonjs",
    "lint": "eslint src/**"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/youtube": "^0.0.47",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1",
    "typescript": "^3.7.5"
  }
}
